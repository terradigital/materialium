<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:version="2">
 <head>
  <meta expr:charser="data:blog.encoding"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
  <title>
   <b:if cond="data:blog.pageName"><data:blog.pageName/> | </b:if>
   <data:blog.title/>
  </title>
  <b:skin>
   <![CDATA [
    /*
    * Author info & styles
    */
   ]]>
  </b:skin>
 </head>
 <body id="page">
  <div class="content-outer">
   <header class="header-outer">
    <b:section class="header-inner" id="header" maxwidgets="1" showaddelement="no">
     <b:widget id="Header1" locked="true" title="Header" type="Header">
      <b:includable id="main">
       <h1 class="blog-title">
        <b:include name="title"/>
        <b:if cond="data:blog.pageType == &quot;item&quot;">
         <span class="title-separator"/>
         <span class="title-page"><data:blog.pageName/></span>
        </b:if>
       </h1>
       <b:if cond="data:blog.isMobile == &quot;false&quot;">
        <form class="search-form" expr:action="data:blog.homepageUrl + &quot;search/&quot;" method="get">
         <input autocomplete="off" autofocus="autofocus" class="search-input" id="s" name="q" placeholder="Search.." type="search"/>
        </form>
       </b:if>
      </b:includable>
      <b:includable id="title">
       <b:if cond="data:blog.url == data:blog.homepageUrl">
        <data:title/>
       <b:else/>
        <a expr:href="data:blog.homepageUrl">
         <data:title/>
        </a>
       </b:if>
      </b:includable>
      <b:includable id="description"/>
     </b:widget>
    </b:section>
   </header>
   <div class="tabs-outer">
    <b:section class="navigation" id="navigation" maxwidgets="1" showaddelement="no">
     <b:widget id="PageList1" locked="true" title="Navigation" type="PageList">
      <b:includable id="main">
       <div class="slider" id="slider">
        <button class="close-button" title="Close menu"/>
        <h2 class="menu-title">
         <data:blog.title/>
        </h2>
        <ul class="menu">
         <b:loop values="data:links" var="link">
          <a expr:href="data:link.href">
           <b:if cond="data:link.isCurrentPage">
            <li class="selected">
             <data:link.title/>
            </li>
           <b:else/>
            <li>
             <data:link.title/>
            </li>
           </b:if>
          </a>
         </b:loop>
        </ul>
        <p class="attribution">
         <data:blog.title/>
         <script>document.write(new Date().getFullYear());</script>
        </p>
       </div>
      </b:includable>
     </b:widget>
    </b:section>
   </div>
   <div class="main-outer">
    <b:section id="main" class="main" showaddelement="yes">
     <b:widget id="Blog1" locked="true" title="Blog Posts" type="Blog">
      <b:includable id="main" var="top">
       <b:if cond="data:mobile == &quot;false&quot;">
        <b:include data="top" name="status-message"/>
        <div class="posts">
         <b:loop values="data:posts" var="post">
          <b:include data="post" name="post"/>
          <b:if cond="data:blog.pageType != &quot;index&quot;">
           <b:if cond="data:post.showThreadedComments">
            <b:include data="post" name="threaded_comments"/>
           <b:else/>
            <b:include data="post" name="comments"/>
           </b:if>
          </b:if>
         </b:loop>
        </div>
        <b:if cond="data.blog.pageType != &quot;static_page&quot;">
         <b:if cond="data:blog.pageType != &quot;error_page&quot;">
          <b:if cond="data:blog.pageType != &quot;item&quot;">
           <b:include name="nextprev"/>
          </b:if>
         </b:if>
        </b:if>
       <b:else/>
        <b:include name="mobile-main"/>
       </b:if>
      </b:includable>
      <b:includable id="backlinkDeleteIcon" var="backlink"/>
      <b:includable id="backlinks" var="post"/>
      <b:includable id="comment-form" var="post"></b:includable>
      <b:includable id="commentDeleteIcon" var="comment"></b:includable>
      <b:includable id="comment_count_picker" var="post"></b:includable>
      <b:includable id="comments" var="post"></b:includable>
      <b:includable id="feedLinks">
       <b:loop values="data:posts" var="post">
        <b:if cond="data:post.allowComments">
         <b:if cond="data:post.feedLinks">
          <b:include data="post.feedLinks" name="feedLinksBody"/>
         </b:if>
        </b:if>
       </b:loop>
      </b:includable>
      <b:includable id="feedLinksBody" var="links">
       <b:loop values="data:links" var="f">
        <a expr:href="data:f.url" expr:type="data:f.mimeType" rel="nofollow" target="_blank">
         <data:feedLinksMsg/> <data:f.name/> (<data:f.feedType/>)
        </a>
       </b:loop>
      </b:includable>
      <b:includable id="iframe_comments" var="post"></b:includable>
      <b:includable id="mobile-index-post" var="post"></b:includable>
      <b:includable id="mobile-main" var="post"></b:includable>
      <b:includable id="mobile-nextprev"></b:includable>
      <b:includable id="mobile-post" var="post"></b:includable>
      <b:includable id="nextprev"></b:includable>
      <b:includable id="post" var="post"></b:includable>
      <b:includable id="postQuickEdit" var="post"/>
      <b:includable id="shareButtons" var="post"></b:includable>
      <b:includable id="status-message"></b:includable>
      <b:includable id="threaded-comment-form" var="post"></b:includable>
      <b:includable id="threaded-comment_js" var="post"></b:includable>
      <b:includable id="threaded-comments" var="post"></b:includable>
     </b:widget>
    </b:section>
   </div>
  </div>
 </body>
</html>